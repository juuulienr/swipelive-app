        <string>L'accès à la caméra est nécessaire pour prendre des photos</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
        <string>L'accès à la galerie est nécessaire pour récupérer des photos</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSLocationWhenInUseUsageDescription">
        <string>L'accès à la localisation est nécessaire pour accéder à votre position</string>
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
        <string>L'accès à la galerie est nécessaire pour ajouter des photos</string>
    </edit-config>
    <config-file parent="NSCameraUsageDescription" platform="ios" target="*-Info.plist">
        <string>Nous avons besoin d'accéder à votre caméra pour diffuser des vidéos en direct</string>
    </config-file>
    <config-file parent="NSMicrophoneUsageDescription" platform="ios" target="*-Info.plist">
        <string>Nous avons besoin d'accéder à votre microphone pour diffuser des vidéos en direct</string>
    </config-file>
