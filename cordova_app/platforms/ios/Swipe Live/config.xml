    <name>Swipe Live</name>
    <description>Application Swipe Live</description>
    <author email="contact@swipelive.app" href="https://swipelive.app">Swipe Live</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <allow-intent href="itms:*" />
